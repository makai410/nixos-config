{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty",
    "source": "$HOME/Pictures/fastfetch_logos/logo.jpg",
    "padding": {
      "top": 1
    },
    "width": 28
  },
  "display": {
    "separator": "  ",
    "color": "38;2;245;194;231",
    "constants": [
      "\u001b[38;2;245;194;231m",
      "\u001b[38;2;235;188;186m",
      
      "\u001b[38;2;246;193;119m",
      "\u001b[38;2;156;207;216m",
      "\u001b[38;2;196;167;231m",
    ],
  },
  "modules": [

    "break",
    {
          "type": "custom",
          "key": "{$5}   ⊹₊⋆☁︎⋆⁺₊ zutomayo::kksk .🌙⊹₊."
    },
    {
      "type": "custom",
      "key": "╭───────────────────────────────────╮",
    },
    {
      "type": "kernel",
      "key": "│ {$2}{$1}  kernel",
      "format": "{$1}{release>22}{$1} │",
    },
    {
      "type": "command",
      "key": "│ {$3}{$1}  uptime",
      "text": "uptime -p | cut -d ' ' -f 2-",
      "format": "{$1}{>22}{$1} │",
    },
    {
      "type": "shell",
      "key": "│ {$4}{$1}  shell ",
      "format": "{$1}{pretty-name>22}{$1} │",
    },
    {
      "type": "command",
      "key": "│ {$5}{$1}  mem   ",
      "text": "free -m | awk 'NR==2{printf \"%.2f GiB / %.2f GiB\",$3/1024,$2/1024}'",
      "format": "{$1}{>22}{$1} │",
    },
    {
      "type": "packages",
      "key": "│ {$2}{$1}  pkgs  ",
      "format": "{$1}{all>22}{$1} │",
    },
    {
      "type": "command",
      "key": "│ {$3}{$1}  user  ",
      "text": "echo $USER",
      "format": "{$1}{>22}{$1} │",
    },
    {
      "type": "command",
      "key": "│ {$4}{$1}  hname ",
      "text": "hostnamectl hostname",
      "format": "{$1}{>22}{$1} │",
    },
    {
      "type": "os",
      "key": "│ {$5}󰻀{$1}  distro",
      "format": "{$1}{pretty-name>22}{$1} │",
    },
    {
      "type": "custom",
      "key": "╰───────────────────────────────────╯",
    },
    "break",
  ],
}
